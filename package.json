{
  "name": "dynamic-web-app",
  "version": "1.0.0",
  "description": "A dynamic web application with Node.js, Express, and SQLite.",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "start:ci": "NODE_ENV=test node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "test:backend": "NODE_ENV=test mocha --require test/setup.js test/backend.test.js --timeout 10000",
    "test:security": "npm audit --audit-level=high",
    "test:load": "artillery run test/performance/load-test.yml",
    "cy:run": "start-server-and-test start:ci http://localhost:3000 \"cypress run\"",
    "cy:open": "start-server-and-test start http://localhost:3000 \"cypress open\"",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "playwright:test": "start-server-and-test start:ci http://localhost:3000 \"playwright test\"",
    "docker:build": "docker build -t dynamic-web-app .",
    "docker:run": "docker run -d -p 3002:3000 --name web-app-container dynamic-web-app",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Me",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "prom-client": "^14.2.0",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "artillery": "^2.0.0",
    "chai": "^4.3.10",
    "cypress": "^13.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.2",
    "start-server-and-test": "^2.0.3",
    "supertest": "^6.3.3"
  }
}
