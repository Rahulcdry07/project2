{
  "name": "dashboard-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/dashboard-app/build/",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "bootstrap": "^5.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.24.1",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:react": "react-scripts test --testPathPattern=src/components/__tests__ --setupFilesAfterEnv=src/test-setup.js --watchAll=false",
    "test:react:watch": "react-scripts test --testPathPattern=src/components/__tests__ --setupFilesAfterEnv=src/test-setup.js",
    "test:react:coverage": "react-scripts test --testPathPattern=src/components/__tests__ --setupFilesAfterEnv=src/test-setup.js --coverage --watchAll=false",
    "test:all:react": "react-scripts test --testPathPattern=src --setupFilesAfterEnv=src/test-setup.js --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:3000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "cypress": "^14.5.2",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^30.0.4",
    "react-scripts": "^5.0.1"
  }
}
